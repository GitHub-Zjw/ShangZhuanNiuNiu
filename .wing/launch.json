{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Launch Wing Player",
			"type": "chrome",
			"request": "launch",
			"file": "index.html",
			"runtimeExecutable": "${execPath}",
			"useBuildInServer": true,
			"sourceMaps": true,
			"webRoot": "${workspaceRoot}",
			"preLaunchTask":"build",
			"port":5926
		},
		{
			"name": "Launch Chrome",
			"type": "chrome",
			"request": "launch",
			"file": "index.html",
            "runtimeExecutable": "C:/Program Files (x86)/Google/Chrome/Application/chrome",
			"useBuildInServer": true,
			"sourceMaps": true,
			"webRoot": "${workspaceRoot}",
			"preLaunchTask":"build",
			"userDataDir":"${tmpdir}",
			"port":5926
		}
	]
}